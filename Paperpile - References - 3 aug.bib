@ARTICLE{Idowu2024-dy,
  title        = {Debiasing Education Algorithms},
  author       = {Idowu, Jamiu Adekunle},
  journaltitle = {International Journal of Artificial Intelligence in Education},
  date         = {2024-01-04},
  doi          = {10.1007/s40593-023-00389-4},
  issn         = {1560-4306,1560-4306},
  abstract     = {This systematic literature review investigates the fairness of
                  machine learning algorithms in educational settings, focusing
                  on recent studies and their proposed solutions to address
                  biases. Applications analyzed include student dropout
                  prediction, performance prediction, forum post classification,
                  and recommender systems. We identify common strategies, such
                  as adjusting sample weights, bias attenuation methods,
                  fairness through un/awareness, and adversarial learning.
                  Commonly used metrics for fairness assessment include ABROCA,
                  group difference in performance, and disparity metrics. The
                  review underscores the need for context-specific approaches to
                  ensure equitable treatment and reveals that most studies found
                  no strict tradeoff between fairness and accuracy. We recommend
                  evaluating fairness of data and features before algorithmic
                  fairness to prevent algorithms from receiving discriminatory
                  inputs, expanding the scope of education fairness studies
                  beyond gender and race to include other demographic
                  attributes, and assessing the impact of fair algorithms on end
                  users, as human perceptions may not align with algorithmic
                  fairness measures.},
  url          = {https://doi.org/10.1007/s40593-023-00389-4},
  urldate      = {2024-04-12},
  file         = {All Papers/I/Idowu 2024 - Debiasing Education Algorithms.pdf},
  keywords     = {Book... Form of Tacit Knowledge to Literate
                  Practice/AIED/International Journal of Artificial Intelligence
                  in Education;\#AIED;literature review},
  language     = {en}
}

