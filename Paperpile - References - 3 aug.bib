  file         = {All Papers/S/Stanoyevitch 2024 - Online assessment in the age of artificial intelligence.pdf},
  keywords     = {Exams \& AI, genAI, LLM:s},
  language     = {en}
}

@ARTICLE{Eachempati2025-gh,
