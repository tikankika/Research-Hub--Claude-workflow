@ARTICLE{Kemmis_undated-ms,
  title    = {On being stirred in to practices},
  author   = {Kemmis, Stephen},
  abstract = {This article aims to give a ‘societist’ (Schatzki, 2003) account
              of ‘learning.’ It offers a new definition of practice, following
              lines suggested by the practice philosophy and theory of Theodore
              Schatzki (1996, 2002, 2010) and by a theory of ‘practice
              architectures’ offered by Kemmis and Grootenboer (2008). Drawing
              on observations of classrooms, schools and a school district, the
              authors aim to defend three claims. First, they argue, insofar as
              people can be said to ‘learn’ anything at all, people learn only
              language games, activities and practices. Second, from a practice
              theory perspective, they argue that the concept ‘learning’ is a
              descriptive concept, not a substantive concept: it describes the
              state of someone’s level of accomplishment in a language game,
              activity or practice; it is not a distinctive process separate
              from participating in the language game or activity or from
              practising the practice. These two claims lead them to conclude,
              third, that to ‘learn’ is to be ‘stirred in’ to language games,
              activities and practices. The authors draw on examples of learning
              and teaching practices observed in classrooms and described in
              interviews, as well as practices of professional development and
              professional learning, and practices of leadership.},
  language = {en}
}

