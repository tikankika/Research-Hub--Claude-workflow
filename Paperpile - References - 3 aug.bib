@ARTICLE{Hiltz2015-hg,
  title    = {Self-Directed Learning: An Approach to Teacher Technology
              Professional Development},
  author   = {Hiltz, Virginia},
  date     = {2015},
  abstract = {Delivery methods for online technology professional development
              (PD), facilitated classes and independent explorations based on
              principles of self-directed learning (SDL), were explored in a
              controlled trial with 46 staff members in a mid-sized K-8 school
              district. Participants engaged with four technology tools via two
              sessions of independent exploration and two sessions of
              facilitated classes. The impacts of these two approaches were
              examined through measures of teacher knowledge, levels of
              technology integration, and teachers’ perceptions of
              effectiveness. Participants showed preference for differentiation
              of PD delivery method based on previous knowledge of the topic,
              time requirements, and engagement. Analysis showed no
              statistically significant difference between delivery method and
              knowledge-gained for five of the individual technology tools that
              were utilized in this study, but did show a statistically
              significant relationship for one tool, indicating a larger
              increase in knowledge via the independent exploration method.
              Teacher self-report data showed a slight preference for the
              independent exploration model, though the relationship was not
              statistically significant. These findings may be practically
              valuable for technology PD designers as they develop technology PD
              for teachers.},
  note     = {CONF papers SITE 3},
  language = {en}
}

@ARTICLE{Loizzo2016-co,
  title        = {{MOOCocracy}: the learning culture of massive open online
                  courses},
  shorttitle   = {MOOCocracy},
  author       = {Loizzo, Jamie and Ertmer, Peggy A},
  journaltitle = {Educational technology research and development: ETR \& D},
  volume       = {64},
  issue        = {6},
  pages        = {1013--1032},
  date         = {2016},
  doi          = {10.1007/s11423-016-9444-7},
  issn         = {1042-1629,1556-6501},
  abstract     = {Massive open online courses (MOOCs) are often examined and
                  evaluated in terms of institutional cost, instructor prestige,
                  number of students enrolled, and completion rates. MOOCs,
                  which are connecting thousands of adult learners from diverse
                  backgrounds, have yet to be viewed from a learning culture
                  perspective. This research used virtual ethnographic methods
                  to investigate the adult learner experience in a MOOC learning
                  culture. Speciﬁcally, authors observed and interviewed twelve
                  adult learners from countries around the world to gain a
                  richer understanding of their online experiences and
                  interactions within a MOOC focused on the social justice topic
                  of human trafﬁcking. Results showed that while a MOOC learning
                  culture has some similarities to traditional distance
                  education environments, it is indeed complex due to the large
                  global scale. Based on the six themes that emerged from the
                  data, the authors present the concept of MOOCocracy—a social
                  learning democracy, as a description of the MOOC learning
                  culture. Implications for MOOC instructional design are also
                  discussed.},
  url          = {http://link.springer.com/10.1007/s11423-016-9444-7},
  urldate      = {2024-04-12},
  note         = {Kan vara central i en slutdiskussion},
  file         = {All Papers/L/Loizzo and Ertmer 2016 - MOOCocracy - the learning culture of massive open online courses.pdf},
